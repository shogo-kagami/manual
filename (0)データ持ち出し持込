ln variables.tf sbkk/variables.tf
ln sbkk/sbkk.auto.tfvars sbkk/01/sbkk.auto.tfvars
ln sbkk/variables_sbkk.tf sbkk/01/variables_sbkk.tf
ln sbkk/terraform.tfvars sbkk/01/terraform.tfvars
ln sbkk/variables.tf sbkk/01/variables.tf
ln sbkk/sbkk.auto.tfvars sbkk/02/sbkk.auto.tfvars
